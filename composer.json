{
  "name": "jazzica/kartevonmorgen",
  "type": "typo3-cms-extension",
  "description": "TYPO3 integration for the kartevonmorgen platform (https://kartevonmorgen.org/)",
  "authors": [
    {
      "name": "Jessica Schlierenkamp",
      "email": "mail@schlierenkamp.de",
      "role": "Developer",
      "homepage": "https://www.schlierenkamp.de/"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "support": {
    "issues": "https://github.com/jazzica/kartevonmorgen/issues"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "typo3/cms-backend": "^10.4",
    "typo3/cms-core": "^10.4",
    "typo3/cms-extbase": "^10.4",
    "typo3/cms-extensionmanager": "^10.4",
    "typo3/cms-fluid": "^10.4",
    "typo3/cms-frontend": "^10.4",
    "typo3/cms-install": "^10.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.0",
    "nimut/testing-framework": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "JS\\Kvm\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JS\\Kvm\\Tests\\": "Tests"
    }
  },
  "replace": {
    "jazzica/kartevonmorgen": "self.version",
    "typo3-ter/kartevonmorgen": "self.version"
  },
  "scripts": {
    "post-autoload-dump": [
      "@prepare-extension-test-structure"
    ],
    "prepare-extension-test-structure": [
      "Nimut\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "kartevonmorgen"
    }
  }
}
